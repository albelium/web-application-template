{
  "name": "backend",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch --env-file=.env ./src/index.ts",
    "server:test": "node --enable-source-maps dist/index.js",
    "server:prd": "node --enable-source-maps dist/index.js",
    "lint": "eslint './src/**/{*.js,*.cjs,*.ts}' --max-warnings 0",
    "lint:fix": "eslint './src/**/{*.js,*.cjs,*.ts}' --fix",
    "generate": "graphql-codegen --config ./src/graphql/codegen.ts",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@apollo/server": "4.10.4",
    "@as-integrations/fastify": "2.1.1",
    "@fastify/compress": "7.0.3",
    "@fastify/cors": "9.0.1",
    "@fastify/helmet": "11.1.1",
    "@fastify/rate-limit": "9.1.0",
    "@pothos/core": "3.41.1",
    "dotenv": "16.4.5",
    "fastify": "4.27.0",
    "graphql": "16.8.1"
  },
  "devDependencies": {
    "@faker-js/faker": "8.4.1",
    "@graphql-codegen/cli": "5.0.2",
    "@graphql-codegen/client-preset": "4.2.5",
    "@graphql-codegen/schema-ast": "4.0.2",
    "@swc/core": "1.5.5",
    "@swc/jest": "0.2.36",
    "@types/jest": "29.5.12",
    "@types/node": "20.12.11",
    "eslint-config-custom": "workspace:*",
    "jest": "29.7.0",
    "ts-node": "10.9.2",
    "tsconfig": "workspace:*",
    "tsx": "4.10.2",
    "typescript": "5.4.5"
  },
  "volta": {
    "extends": "../../package.json"
  }
}
