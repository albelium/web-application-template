{
  "name": "ui",
  "private": true,
  "main": "dist/index.mjs",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.mjs"
    }
  },
  "scripts": {
    "panda:gen": "panda codegen",
    "ship": "panda ship src/**/*.tsx --outfile dist/panda.json",
    "build:style": "pnpm panda:gen && pnpm ship",
    "build": "pnpm build:style && vite build && tsc --project tsconfig.build.json",
    "generate:component": "turbo gen react-component",
    "lint": "eslint './src/**/{*.js,*.jsx,*.ts,*.tsx}' --max-warnings 0",
    "lint:fix": "eslint './src/**/{*.js,*.jsx,*.ts,*.tsx}' --fix",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test": "vitest run"
  },
  "devDependencies": {
    "@pandacss/dev": "0.29.1",
    "@storybook/addon-essentials": "^7.6.12",
    "@storybook/addon-interactions": "^7.6.12",
    "@storybook/addon-links": "^7.6.12",
    "@storybook/addon-onboarding": "^1.0.11",
    "@storybook/blocks": "^7.6.12",
    "@storybook/builder-vite": "7.6.12",
    "@storybook/react": "^7.6.12",
    "@storybook/react-vite": "^7.6.12",
    "@storybook/test": "^7.6.12",
    "@testing-library/react": "14.2.1",
    "@turbo/gen": "1.12.2",
    "@types/node": "20.11.14",
    "@types/react": "18.2.51",
    "@types/react-dom": "18.2.18",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint-config-custom": "workspace:*",
    "eslint-plugin-storybook": "0.6.15",
    "happy-dom": "13.3.8",
    "panda-preset": "workspace:*",
    "react": "18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^7.6.12",
    "icon": "workspace:*",
    "tsconfig": "workspace:*",
    "tsup": "^8.0.1",
    "typescript": "5.3.3",
    "vitest": "1.2.2",
    "vite": "^5.0.12"
  },
  "dependencies": {
    "@radix-ui/react-slot": "1.0.2"
  },
  "volta": {
    "extends": "../../package.json"
  }
}
