{
  "name": "icon",
  "private": true,
  "main": "index.mjs",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "svgr -d dist/icons -- assets && tsup dist/icons/index.ts --format cjs,esm --dts --out-dir dist"
  },
  "devDependencies": {
    "@types/react": "18.2.75",
    "@svgr/cli": "8.1.0",
    "react": "^18.2.0",
    "tsup": "^8.0.2",
    "typescript": "5.4.4",
    "tsconfig": "workspace:*"
  },
  "volta": {
    "extends": "../../package.json"
  }
}
